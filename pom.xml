		<netty4.version>4.0.36.Final</netty4.version>
		<!-- IMPORTANT: In the past Jersey have split out classes into separate 
			submodules and broken the previous module dependencies at runtime -->
		<jersey.version>1.19.1</jersey.version>
		<janino.version>2.7.8</janino.version>
	</properties>
