		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<rdf4j.version>2.0M1</rdf4j.version>

		<!-- IMPORTANT: Sensitive to changes at patch(!!!) version level, must 
			match Spark Cluster Server version exactly to function -->
		<akka.version>2.3.11</akka.version>
		<!-- IMPORTANT: Sensitive to changes at the minor version level, must match 
			Spark Cluster Server version at minor level to function -->
		<scala-lib.version>2.10.6</scala-lib.version>
		<json4s.version>3.3.0</json4s.version>
		<slf4j.version>1.7.21</slf4j.version>
